// Manages a specific log forwarder.
resource LogForwarder {
	// Retrieves the details of the log forwarder.
	method Get {
		out Body LogForwarder
	}

	// Updates the log forwarder.
	method Update {
		in out Body LogForwarder
	}

	// Deletes the log forwarder.
	method Delete {
	}
}
